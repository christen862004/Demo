@model List<Department>
@{
    ViewData["Title"] = "Index";
}
<a href="/Department/New" class="btn btn-success">New DEpartment</a>
<h1>Index</h1>
<table class="table table-bordered table-hover">
@foreach (var item in Model)
{
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
    </tr>
}
</table>
